   "source": [
    "skk = np.linalg.inv(X.T @ X)[1][1]\n",
    "s = standard_error(y, X @ beta)\n",
    "t_alpha = search(.01, SAMPLE_SIZE - 2)\n",
    "beta_hat = t_alpha * ((s ** 2) * skk) ** (1/2)\n",
    "\n",
    "print(t_alpha)\n",
    "print(beta_hat)\n",
